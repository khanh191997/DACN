@model List<Models.Model.Supplier>
<div class="aside">
    <h3 class="aside-title">Brand</h3>
    <div class="checkbox-filter">
        @foreach (var item in Model)
        {
            <div class="product-category">
                <ul>
                    <li>
                        <a href="/san-pham/@item.Name-@item.ID">@item.Name</a>
                    </li>
                </ul>

            </div>
        }

    </div>
</div>